<?xml version="1.0" encoding="utf-8"?>


<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">


    <!-- Background color -->
    <solid android:color="#04fca9" />
    <!-- view border color and width -->
    <stroke
        android:width="1dp"
        android:color="#dc0e0e0e" >
    </stroke>

    <!-- Padding -->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"    >
    </padding>

    <!-- Corner radius -->
   <corners
        android:radius="6dp"   >
    </corners>



</shape>